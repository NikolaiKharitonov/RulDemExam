<Window x:Class="RulDemka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RulDemka"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="icon.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Margin="0,0,-0.4,0" Background="#FF76E383">
            <Button Name="btnBack" HorizontalAlignment="Right" Content="Выход" Click="btnBack_Click"></Button>
            <Image HorizontalAlignment="Left" Margin="5" Width="45" Height="45" Source="logo.png"></Image>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"></Frame>
    </Grid>
</Window>
